{
  "manifest_version": 2,
  "name": "Charger Warning",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "warning.png"
  },
  "permissions": ["idle", "tabs"],
  "web_accessible_resources": ["style.css"]
}
